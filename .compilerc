{
  "env": {
    "development": {
      "application/javascript": {
        "presets": ["env", "react"],
        "sourceMaps": "inline"
      },
      "text/typescript": {
        "target": "es6",
        "lib": ["es2015", "es2016", "dom"],
        "module": "commonjs",
        "esModuleInterop": true,
        "jsx": "react",
        "sourceMap": true,
        "inlineSourceMap": true,
        "inlineSources": true
      }
    },
    "production": {
      "application/javascript": {
        "presets": ["env", "react"]
      },
      "text/typescript": {
        "target": "es6",
        "lib": ["es2015", "es2016", "dom"],
        "module": "commonjs",
        "esModuleInterop": true,
        "jsx": "react"
      }
    }
  }
}
